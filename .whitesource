{
  "settingsInheritedFrom": "Dima2021/whitesource-config@master",
  "scanSettings": {
    "enableReachability": true
  },
  "checkRunSettings": {
    "strictMode": "warning"
  },
  "scanSettingsSAST": {
    "enableScan": "true",
    "scanPullRequests": true
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
    "config:base",
      "github>whitesource/merge-confidence:beta",
      "github>mend-toolkit/mend-examples//Repo-Integration/Renovate/smart-merge"
    ]
  },
  "imageSettings": {
    "imageTracing": {
      "enableImageTracingPR": true,
      "addDockerfilePath": true,
      "addRepositoryCoordinates": true
    }
  }
}
